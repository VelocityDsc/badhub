-- Scripts:

local function XUJFMSI_fake_script() -- TextButton.LocalScript 
	local script = Instance.new('LocalScript', TextButton)

	
	script.Parent.MouseButton1Click:Connect(function()
			local goddamn = script.Parent.Parent.TextBox.Text
	
			script.Parent.Parent.UsernameText.Text = goddamn
			script.Parent.Parent.SubText.Text = "You are viewing the inventory of"
			script.Parent.Parent.ImageLabel.Image = "https://www.roblox.com/headshot-thumbnail/image?userId=" .. game:GetService("Players")[goddamn].UserId .. "&width=180&height=180"
	
			for _,again in pairs(game:GetService("CoreGui").InvView.BackgroundFrame.InvView.ScrollingFrame:GetChildren()) do
				if again:IsA("Frame") then
					again:Destroy()
				end
			end
	
			wait(0.1)
	
			for _,skaigs in pairs(game:GetService("Players")[goddamn].trails:GetChildren()) do
				local Frame = Instance.new("Frame")
				local UICorner = Instance.new("UICorner")
				local TextLabel = Instance.new("TextLabel")
				local UIGradient = Instance.new("UIGradient")
	
	
	
				Frame.Parent = game:GetService("CoreGui").InvView.BackgroundFrame.InvView.ScrollingFrame
				Frame.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
				Frame.Size = UDim2.new(0, 100, 0, 100)
	
				UICorner.Parent = Frame
	
				TextLabel.Parent = Frame
				TextLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
				TextLabel.BackgroundTransparency = 1.000
				TextLabel.Position = UDim2.new(0.047110945, 0, 0.304586798, 0)
				TextLabel.Size = UDim2.new(0.56497246, 0, 0.35826391, 0)
				TextLabel.ZIndex = 2
				TextLabel.Font = Enum.Font.GothamBold
				TextLabel.Text = skaigs.Name
				TextLabel.TextColor3 = Color3.fromRGB(222, 222, 222)
				TextLabel.TextSize = 14.000
				TextLabel.TextXAlignment = Enum.TextXAlignment.Left
	
				UIGradient.Color = ColorSequence.new{ColorSequenceKeypoint.new(0.00, Color3.fromRGB(255, 255, 255)), ColorSequenceKeypoint.new(1.00, Color3.fromRGB(207, 207, 207))}
				UIGradient.Rotation = 90
				UIGradient.Parent = TextLabel
			end
	end)
end
coroutine.wrap(XUJFMSI_fake_script)()
local function QFHSDX_fake_script() -- InvView_2.Dragify 
	local script = Instance.new('LocalScript', InvView_2)

	local UIS = game:GetService("UserInputService")
	function dragify(Frame)
	    dragToggle = nil
	    local dragSpeed = 0.50
	    dragInput = nil
	    dragStart = nil
	    local dragPos = nil
	    function updateInput(input)
	        local Delta = input.Position - dragStart
	        local Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + Delta.X, startPos.Y.Scale, startPos.Y.Offset + Delta.Y)
	        game:GetService("TweenService"):Create(Frame, TweenInfo.new(0.30), {Position = Position}):Play()
	    end
	    Frame.InputBegan:Connect(function(input)
	        if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) and UIS:GetFocusedTextBox() == nil then
	            dragToggle = true
	            dragStart = input.Position
	            startPos = Frame.Position
	            input.Changed:Connect(function()
	                if input.UserInputState == Enum.UserInputState.End then
	                    dragToggle = false
	                end
	            end)
	        end
	    end)
	    Frame.InputChanged:Connect(function(input)
	        if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
	            dragInput = input
	        end
	    end)
	    game:GetService("UserInputService").InputChanged:Connect(function(input)
	        if input == dragInput and dragToggle then
	            updateInput(input)
	        end
	    end)
	end
	
	dragify(script.Parent)
end
coroutine.wrap(QFHSDX_fake_script)()
